config_version=4

[application]

config/name="OblivionTale"
run/main_scene="res://other/opening.tscn"
config/use_custom_user_dir=true
config/custom_user_dir_name="OblivionTaleData"
config/icon="res://icon.png"
config/windows_native_icon="res://Francisco.ico"

[autoload]

Achievement="*res://scripts/achievement.gd"
Global="*res://scripts/global.gd"
GlobalTimer="*res://other/Timer.tscn"
SaveLoader="*res://scripts/save_loader.gd"
Config="*res://scripts/config.gd"
SceneChanger="*res://other/scene_changer.tscn"
AudioSFX="*res://other/AudioSFX.tscn"

[debug]

settings/fps/force_fps=60

[display]

window/size/width=1280
window/size/height=720
window/size/test_width=640
window/size/test_height=360
window/vsync/vsync_via_compositor=true
mouse_cursor/custom_image="res://images/GUI/cursor.png"
window/stretch/mode="2d"

